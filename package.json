{
  "name": "Learning Map",
  "version": "1.5.0",
  "description": "Learning Map Application",
  "engines": {
    "node": ">=4"
  },
  "main": "./server/server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node ./server/server.js",
    "dev:server": "nodemon server/server.js",
    "validate-webpack:dev": "webpack-validator webpack.config.js --env.dev",
    "validate-webpack:prod": "webpack-validator webpack.config.js --env.prod",
    "clean-www": "rm -rf ./front/www/",
    "predev": "npm run clean-www",
    "dev": "webpack-dev-server --inline --hot --env.dev --history-api-fallback --open --port 3000 --content-base front/www/",
    "prebuild": "npm run clean-www",
    "prebuild:prod": "npm run clean-www",
    "build": "webpack --env.dev",
    "build:prod": "webpack --env.prod --optimize-minimize -p",
    "lint": "eslint .",
    "setup": "npm install && npm run validate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nudoru/learning-map.git"
